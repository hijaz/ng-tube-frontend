pipeline {	 
	agent any	 
		stages {     	 
    		stage('Starting Build') {
        			steps {               	 
            			echo 'Sarting the build'          
        			}     	 
    		}     	 
    		stage('Building') {          	 
        			steps {               	 
            			sh 'npm install'          	
        			}     
    		}	
	}
}